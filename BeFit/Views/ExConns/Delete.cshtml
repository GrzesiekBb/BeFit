@model BeFit.Models.ExConn

@{
    ViewData["Title"] = "Delete";
}

<h3>Czy na pewno chcesz usunąć ten wpis?</h3>

<table class="table table-bordered">
    <tr><th>Ćwiczenie</th><td>@Model.ExType?.Name</td></tr>
    <tr><th>Start</th><td>@Model.SessionInfo?.Start</td></tr>
    <tr><th>Koniec</th><td>@Model.SessionInfo?.End</td></tr>
    <tr><th>Serie</th><td>@Model.Sets</td></tr>
    <tr><th>Powtórzenia</th><td>@Model.RepsPerSet</td></tr>
    <tr><th>Obciążenie</th><td>@Model.Load</td></tr>
</table>

<form asp-action="DeleteConfirmed">
    <input type="hidden" asp-for="ConnId" />
    <input type="submit" value="Delete" class="btn btn-danger" />
</form>

<a asp-action="Index">Powrót</a>
